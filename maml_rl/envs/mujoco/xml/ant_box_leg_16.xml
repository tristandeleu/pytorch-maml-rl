          <body pos="0.2 -0.2 0">
            <joint axis="1 1 0" name="ankle_4" pos="0.0 0.0 0.0" range="30 70" type="hinge" />
            <geom fromto="0.0 0.0 0.0 0.4 -0.4 0.0" name="fourth_ankle_geom" size="0.08" type="capsule" />
          </body>
        </body>
      </body>

      <body euler="0 90 0" name="line" pos="-1.0 0 0">
        <joint axis="0 1 0" name="line connection" pos="0 0 1.0 " range="-30 30" type="hinge" />
        <geom mass="0.001" name="line" rgba="0.2 0.3 0.4 1" size="0.03 1.0" type="cylinder" />
          <body euler="0 0 0" name="case" pos="0 0 -1.0">
