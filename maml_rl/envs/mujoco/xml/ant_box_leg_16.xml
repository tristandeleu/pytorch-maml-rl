          <body pos="-0.2 -0.2 0">
            <joint axis="-1 1 0" name="ankle_3" pos="0.0 0.0 0.0" range="-70 -30" type="hinge" />
            <geom fromto="0.0 0.0 0.0 -0.4 -0.4 0.0" mass="0.62" name="third_ankle_geom" rgba="0.14 0.14 0.14 1" size="0.08" type="capsule" />
